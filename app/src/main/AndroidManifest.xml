<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sixplusone.uwcoursecalendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.sixplusone.uwcoursecalendar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sixplusone.uwcoursecalendar.Arts" />
        <activity android:name="com.example.sixplusone.uwcoursecalendar.Science" />
        <activity android:name="com.example.sixplusone.uwcoursecalendar.Math" />
        <activity android:name="com.example.sixplusone.uwcoursecalendar.Environment" />
        <activity android:name="com.example.sixplusone.uwcoursecalendar.Engineering" />
        <activity android:name="com.example.sixplusone.uwcoursecalendar.AppliedHealthSci" />
        <activity android:name="com.example.sixplusone.uwcoursecalendar.CourseDetails"></activity>
    </application>

</manifest>